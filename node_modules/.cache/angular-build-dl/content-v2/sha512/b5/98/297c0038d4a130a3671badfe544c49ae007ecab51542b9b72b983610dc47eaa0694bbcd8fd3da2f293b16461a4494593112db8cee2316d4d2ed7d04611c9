r\n           if(data.status == 0)\r\n           {\r\n             this.user = data.user;\r\n             console.log(data);\r\n           }\r\n           else\r\n           {\r\n             console.log(data);\r\n           }\r\n           this.global.loadingDismiss();\r\n          }, error => {\r\n           console.log(error);\r\n      });\r\n\r\n    });\r\n  }\r\n\r\n}\r\n","export default \"\\n/*# sourceMappingURL=data:application/json;base64,eyJ2ZXJzaW9uIjozLCJzb3VyY2VzIjpbXSwibmFtZXMiOltdLCJtYXBwaW5ncyI6IiIsImZpbGUiOiJ1c2VyLnBhZ2Uuc2NzcyJ9 */\";","import { NgModule } from '@angular/core';\r\nimport { Routes, RouterModule } from '@angular/router';\r\n\r\nimport { UserPage } from './user.page';\r\n\r\nconst routes: Routes = [\r\n  {\r\n    path: '',\r\n    component: UserPage\r\n  }\r\n];\r\n\r\n@NgModule({\r\n  imports: [RouterModule.forChild(routes)],\r\n  exports: [RouterModule],\r\n})\r\nexport class UserPageRoutingModule {}\r\n"],"sourceRoot":"webpack:///"}